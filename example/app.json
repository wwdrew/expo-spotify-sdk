{
  "expo": {
    "name": "expo-spotify-sdk-example",
    "scheme": "expo-spotify-sdk-example",
    "slug": "expo-spotify-sdk-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.spotifysdk.example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "expo.modules.spotifysdk.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["../app.plugin.js", {
        "host": "authenticate",
        "scheme": "expo-spotify-sdk-example",
        "clientID": "<your-client-id>",
        "tokenSwapURL": "<your-token-swap-url>"
      }]
    ]
  }
}
